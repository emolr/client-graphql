{
  "name": "graphql-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-schema": "^1.2.4",
    "apollo-server": "^2.10.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "concurrently \"npm run generate:watch\" \"react-scripts start\"",
    "build": "npm run generate && react-scripts build",
    "test": "npm run generate && react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config ./codegen.yml",
    "generate:watch": "npm run generate -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "graphql-tag-schema": "^1.0.5"
  }
}
