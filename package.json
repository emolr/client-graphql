{
  "name": "graphql-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-components": "^4.0.0",
    "@apollo/react-hoc": "^4.0.0",
    "@apollo/react-hooks": "^4.0.0",
    "@graphql-codegen/schema-ast": "^2.1.0",
    "@graphql-tools/schema": "^8.1.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-schema": "^1.2.5",
    "apollo-server": "^3.1.2",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "concurrently": "^6.2.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "graphql-tools": "^8.1.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "concurrently \"npm run generate:watch\" \"react-app-rewired start\"",
    "build": "npm run generate && react-app-rewired build",
    "test": "npm run generate && react-app-rewired test",
    "eject": "react-app-rewired eject",
    "generate": "graphql-codegen --config ./codegen.yml",
    "generate:watch": "npm run generate -- --watch",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/typescript": "^2.1.1",
    "@graphql-codegen/typescript-document-nodes": "^2.1.1",
    "@graphql-codegen/typescript-operations": "^2.1.1",
    "@graphql-codegen/typescript-react-apollo": "^3.1.1",
    "@graphql-codegen/typescript-resolvers": "^2.1.1",
    "@types/classnames": "^2.3.0",
    "graphql-tag-schema": "^1.0.6",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "node-sass": "^7.0.0",
    "prettier": "^2.3.2"
  }
}
