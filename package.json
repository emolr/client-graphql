{
  "name": "graphql-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@graphql-codegen/typescript-document-nodes": "^1.12.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-schema": "^1.2.4",
    "apollo-server": "^2.10.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^4.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "start": "concurrently \"npm run generate:watch\" \"react-scripts start\"",
    "build": "npm run generate && react-scripts build",
    "test": "npm run generate && react-scripts test",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config ./codegen.yml",
    "generate:watch": "npm run generate -- --watch",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "@types/classnames": "^2.2.9",
    "graphql-tag-schema": "^1.0.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
